{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "rootDir": "ts", 
    "outDir": "wwwroot/dist",
    "strict": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [ "ts/**/*.ts" ], 
  "exclude": [ "node_modules" ]
}
